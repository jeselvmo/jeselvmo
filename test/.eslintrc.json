{
	"parserOptions": {
		"ecmaVersion": 5,
		"sourceType": "module"
	},
	"rules": {
		"no-var": 0,
		"vars-on-top": 0,
		"prefer-arrow-callback": 0,
		"object-shorthand": 0,
		"quote-props": 0,
		"no-plusplus": 0,
		"no-bitwise": 0,
		"object-curly-spacing": 0,
		"padded-blocks": 0,
		"comma-dangle": 0
	}
}
