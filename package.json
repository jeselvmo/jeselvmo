{
  "name": "jeselvmo",
  "version": "2.0.0",
  "description": "JavaScript utility library",
  "homepage": "https://github.com/jeselvmo/jeselvmo.js",
  "author": "kecoyo <kecoyo@163.com>",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src test",
    "lint:fix": "eslint --fix src test",
    "clean:node": "rm -rf index.js lib",
    "clean:browser": "rm -rf jeselvmo*",
    "clean": "npm run clean:node && npm run clean:browser",
    "minify": "uglifyjs jeselvmo.js -o jeselvmo.min.js  --compress --mangle --comments /Copyright/",
    "build:browser": "rollup -c && npm run minify",
    "build:node": "babel src -d .",
    "build": "npm run build:browser && npm run build:node",
    "test": "mocha --require @babel/register --reporter dot"
  },
  "files": [
    "index.js",
    "lib",
    "README.md",
    "LICENCE",
    "jeselvmo.js",
    "jeselvmo.min.js"
  ],
  "keywords": [
    "jeselvmo",
    "validate"
  ],
  "contributors": [
    {
      "name": "jeselvmo",
      "url": "https://github.com/jeselvmo"
    }
  ],
  "bugs": {
    "url": "https://github.com/jeselvmo/jeselvmo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeselvmo/jeselvmo.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.2",
    "babel-plugin-add-module-exports": "^1.0.2",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "rollup": "^1.16.4",
    "rollup-plugin-babel": "^4.3.3",
    "uglify-js": "^3.6.0"
  },
  "engines": {
    "node": ">= 0.10"
  },
  "license": "MIT"
}