{
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module"
	},
	"rules": {
		"import/newline-after-import": 0,
		"import/no-extraneous-dependencies": 0,
		"no-console": 0,
		"no-plusplus": 0,
		"no-bitwise": 0,
		"no-prototype-builtins": 0,
		"no-useless-escape": 0,
		"no-underscore-dangle": 0,
		"quotes": 0,
		"quote-props": 0,
		"semi": 0,
		"padded-blocks": 0,
		"no-multi-spaces": 0,
		"no-continue": 0,
		"guard-for-in": 0,
		"comma-dangle": 0,
		"no-trailing-spaces": 0,
		"class-methods-use-this": 0,
		"spaced-comment": 0,
		"dot-notation": 0,
		"brace-style": 0,
		"prefer-template": 0,
		"prefer-arrow-callback": 0,
		"prefer-rest-params": 0
	}
}
