{
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module"
	},
	"rules": {
		"no-console": 0,
		"no-plusplus": 0,
		"no-bitwise": 0,
		"no-prototype-builtins": 0,
		"no-useless-escape": 0,
		"quotes": 0,
		"semi": 0,
		"padded-blocks": 0,
		"no-multi-spaces": 0,
		"comma-dangle": 0,
		"no-trailing-spaces": 0,
		"class-methods-use-this": 0,
		"spaced-comment": 0,
		"dot-notation": 0,
		"brace-style": 0,
		"prefer-template":0
	}
}
