{
  "extends": ["eslint:recommended", "plugin:import/recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "no-useless-escape": 0,
    "no-prototype-builtins": 0
  }
}
